{
  "manifest_version": 3,
  // 插件的名称
  "name": "MyDLChromeExt",
  // 插件的版本
  "version": "1.0.1",
  // 插件描述
  "description": "MyDLChromeExt",
  // 会一直常驻的后台JS或后台页面
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon/logo.png",
    "48": "icon/logo.png",
    "128": "icon/logo.png"
  },
  // 权限申请
  "permissions": [
    "cookies",
    "webRequest",
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}